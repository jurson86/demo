<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.icomwell.anta.persist.dao.WeibuWeixinUserDao">

    <select id="selectWeixinUserByAppUserId" resultType="WeibuWeixinUser">
        select *
        from weibu_weixin_user
        where app_user_id = #{appUserId}
    </select>

</mapper>
